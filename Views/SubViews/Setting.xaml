                                                            <Label Content="layoutPixPadding:" FontSize="15" BorderThickness="0" Background ="Transparent"/>
                                                            <Border>
                                                                <ListBox ItemsSource="{Binding layoutPixPadding}" Padding="0" Margin="0" Background="Transparent">
                                                                    <ListBox.ItemsPanel>
                                                                        <ItemsPanelTemplate>
                                                                            <WrapPanel/>
                                                                        </ItemsPanelTemplate>
                                                                    </ListBox.ItemsPanel>
                                                                    <ListBox.ItemTemplate>
                                                                        <DataTemplate>
                                                                            <Border Width="30" Padding="2" Margin="-8,0,-8,0">
                                                                                <Label Margin="0" Padding="5,2,5,2" Content="{Binding ., UpdateSourceTrigger=PropertyChanged}"/>
                                                                            </Border>
                                                                        </DataTemplate>
                                                                    </ListBox.ItemTemplate>
                                                                </ListBox>
                                                            </Border>
                                                        </StackPanel>
                                                        <StackPanel Orientation="Horizontal">
                                                            <Label Content="imageTransScaleX:" FontSize="15" BorderThickness="0" Background ="Transparent"/>
                                                            <Label Content="{Binding Path=imageTransScaleX}" FontSize="15" Background ="LightGray"/>
                                                        </StackPanel>
                                                        <StackPanel Orientation="Horizontal">
                                                            <Label Content="imageTransScaleY:" FontSize="15" BorderThickness="0" Background ="Transparent"/>
                                                            <Label Content="{Binding Path=imageTransScaleY}" FontSize="15" Background ="LightGray"/>
                                                        </StackPanel>
                                                        <StackPanel Orientation="Horizontal">
                                                            <Label Content="imageTransRotate:" FontSize="15" BorderThickness="0" Background ="Transparent"/>
                                                            <Label Content="{Binding Path=imageTransRotate}" FontSize="15" Background ="LightGray"/>
                                                        </StackPanel>
                                                        <StackPanel Orientation="Horizontal">
                                                            <Label Content="projectorPixScaleX:" FontSize="15" BorderThickness="0" Background ="Transparent"/>
                                                            <Label Content="{Binding Path=projectorPixScaleX}" FontSize="15" Background ="LightGray"/>
                                                        </StackPanel>
                                                        <StackPanel Orientation="Horizontal">
                                                            <Label Content="projectorPixScaleY:" FontSize="15" BorderThickness="0" Background ="Transparent"/>
                                                            <Label Content="{Binding Path=projectorPixScaleY}" FontSize="15" Background ="LightGray"/>
                                                        </StackPanel>
                                                        <StackPanel Orientation="Horizontal">
                                                            <Label Content="holeOffsetX:" FontSize="15" BorderThickness="0" Background ="Transparent"/>
                                                            <Label Content="{Binding Path=holeOffsetX}" FontSize="15" Background ="LightGray"/>
                                                        </StackPanel>
                                                        <StackPanel Orientation="Horizontal">
                                                            <Label Content="holeOffsetY:" FontSize="15" BorderThickness="0" Background ="Transparent"/>
                                                            <Label Content="{Binding Path=holeOffsetY}" FontSize="15" Background ="LightGray"/>
                                                        </StackPanel>
                                                        <StackPanel Orientation="Horizontal">
                                                            <Label Content="holeRz:" FontSize="15" BorderThickness="0" Background ="Transparent"/>
                                                            <Label Content="{Binding Path=holeRz}" FontSize="15" Background ="LightGray"/>
                                                        </StackPanel>
                                                        <StackPanel Orientation="Horizontal">
                                                            <Label Content="holeRadius:" FontSize="15" BorderThickness="0" Background ="Transparent"/>
                                                            <Label Content="{Binding Path=holeRadius}" FontSize="15" Background ="LightGray"/>
                                                        </StackPanel>
                                                        <StackPanel Orientation="Horizontal">
                                                            <Label Content="exposeDAC:" FontSize="15" BorderThickness="0" Background ="Transparent"/>
                                                            <Label Content="{Binding Path=exposeDAC}" FontSize="15" Background ="LightGray"/>
                                                        </StackPanel>
                                                        <StackPanel Orientation="Horizontal">
                                                            <Label Content="exposeLight:" FontSize="15" BorderThickness="0" Background ="Transparent"/>
                                                            <Label Content="{Binding Path=exposeLight}" FontSize="15" Background ="LightGray"/>
                                                        </StackPanel>
                                                    </StackPanel>
                                                </ScrollViewer>
                                            </Border>
                                        </DataTemplate>
                                    </ListView.ItemTemplate>
                                </ListView>

                                <StackPanel Orientation="Vertical" Margin="0,5,0,0">

                                </StackPanel>
                            </Grid>
                        </TabItem>
                        <TabItem
                            Background="Transparent"
                            FontSize="18"
                            Header="UserDB">
                            <Grid Margin="0,10,0,0" Background="SlateGray">
                                <StackPanel Orientation="Vertical" Margin="0,5,0,0">
                                    <Button Content="ImportUserInfo"
                                            Command="{Binding UserDBCommand}" CommandParameter="ImportUserInfo"></Button>
                                </StackPanel>
                            </Grid>
                        </TabItem>
                    </TabControl>
                </Grid>
            </StackPanel>
        </Border>
        <Button
            Width="20"
            Height="20"
            Margin="20"
            Padding="1"
            HorizontalAlignment="Right"
            VerticalAlignment="Top"
            hc:BorderElement.CornerRadius="5"
            hc:IconElement.Geometry="{StaticResource i_Close}"
            Background="Transparent"
            BorderThickness="0"
            Command="{Binding CloseCommand}" />
    </Grid>

</UserControl>
